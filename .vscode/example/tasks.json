{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "serve:web",
      "type": "npm",
      "script": "start",
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": {
            "regexp": ""
          },
          "background": {
            "beginsPattern": ".+Building for",
            "endsPattern": ".+App running at:",
            "activeOnStart": true
          }
        }
      ]
    },
    {
      "label": "build",
      "type": "process",
      "command": "make",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "options": {
        "env": {}
      }
    },
    {
      "label": "build:docs",
      "type": "process",
      "command": "make",
      "args": ["docs"]
    },
    {
      "type": "npm",
      "script": "codegen:graphql",
      "problemMatcher": []
    }
  ]
}
