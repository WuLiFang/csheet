  "configurations": [
    {
      "name": "Launch Server",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/csheet",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "serve:web",
      "env": {
        "CGO_ENABLED": "0",
        "CSHEET_ADDRESS": "localhost:8000",
        "CSHEET_ENV": "development"
      }
    }
  ]
